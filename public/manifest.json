{
  "manifest_version": 2,
  "name": "CI Tracker",
  "version": "0.0.3",
  "permissions": ["storage", "*://*.youtube.com/*"],
  "background": {"scripts": ["background.js"]},
  "content_scripts": [{
        "matches": ["*://*.youtube.com/*"], 
        "js": ["track-youtube.js"],
        "run_at": "document_start"
    }],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icons/icon-48.png"
  }
}
